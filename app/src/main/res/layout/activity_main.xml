        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:id="@+id/noi_dung_chinh"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        </LinearLayout>


    </ScrollView>


</LinearLayout>
